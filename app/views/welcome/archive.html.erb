<%
  meta title: "Top Content for #{params[:date]}",
       description: 'Top Jokes, articles, images, stories and memes from the web',
       keywords: ['Jokes', 'Funny Articles', 'Trending Humor', 'Funny Pictures']
%>

<% if @listings.count > 0 %>
  <div id='outer-content-wrapper'>
    <%= will_paginate @listings, renderer: BootstrapPagination::Rails %>

    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Content Archive for <%= params[:date] %> from All Sources</h3>
      </div>
      <%= render 'listing_panel_body' %>
    </div>
  </div>
  <%= will_paginate @listings, renderer: BootstrapPagination::Rails %>
<% else %>
  <div id='inner-content-wrapper'>
    <%= image_tag 'derp.jpg' %>
  </div>
<% end %>
